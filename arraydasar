public class arraydasar {
     public static void main(String[] args) {
    String[] domba = {"merino", "birma", "dorper", "krepek"};
    System.out.println("domba pertama:"+domba[0]);
    System.out.println("domba kedua:"+domba[1]);
    System.out.println("domba ketiga:"+domba[2]);
    System.out.println("domba keempat:"+domba[3]);
     }
}
